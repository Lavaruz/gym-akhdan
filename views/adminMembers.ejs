<%- include('./layouts/header'); -%>
<main class="d-flex">
  <%- include('./layouts/sidebar'); -%>
  <section id="main-menu" class="col">
    <div class="d-flex justify-content-between mb-4">
      <div class="">
        <h2>Members</h2>
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item active" aria-current="page">Members</li>
          </ol>
        </nav>
      </div>

      <div class="d-flex justify-content-end mb-5 gap-3">
        <!-- <button type="button" class="btn btn-primary">
          <i class="uil uil-clipboard-alt"></i> Cetak Laporan
        </button> -->
        <a href="/members/tambah">
          <button type="button" class="btn btn-success">
            <i class="uil uil-user-plus"></i> Tambah Member
          </button>
        </a>
        <form id="form-delete">
          <button type="submit" class="btn btn-danger hapus-button" disabled>
            <i class="uil uil-trash-alt"></i> Hapus
          </button>
        </form>
      </div>
    </div>

    <table id="member-table">
      <thead>
        <th>No</th>
        <th>Nama</th>
        <th>Tanggal Daftar</th>
        <th>Tanggal Mulai</th>
        <th>Tanggal Berakhir</th>
        <th>Status</th>
        <th></th>
        <th><input type="checkbox" name="" id="select-all" /></th>
      </thead>
    </table>
  </section>
</main>
<%- include('./layouts/footer'); -%>

<script>
  $(".nav-dashboard").removeClass("active");
  $(".nav-members").addClass("active");
</script>
<script src="/js/adminMembers.js"></script>
